        <activity
            android:name=".activity.personaldata.PersonalDataOperatorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personaldata.PersonalDataCertificatesActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personaldata.PersonalDataBankCardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.personaldata.PersonalDataUploadActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SetUp.SetUPActivity"
