<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_light_grey"
    android:orientation="vertical">

    <include
        layout="@layout/tab_activity_fra"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common_48dp" />

    <RelativeLayout
        android:id="@+id/set_up_about"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_10dp"
        android:background="@drawable/bg_fillet_shadow_selector"
        android:padding="@dimen/common_18dp">

        <com.jsy.jsydemo.control.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:text="@string/name_loan_set_up_about"
            android:textColor="@color/common_loan_personal"
            android:textSize="@dimen/common_20dp" />


        <ImageView
            android:layout_width="@dimen/common_20dp"
            android:layout_height="@dimen/common_20dp"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:paddingLeft="@dimen/common_10dp"
            android:src="@drawable/bg_personal_right_selector" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/set_up_password"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_10dp"
        android:background="@drawable/bg_fillet_shadow_selector"
        android:padding="@dimen/common_18dp">

        <com.jsy.jsydemo.control.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:text="@string/name_loan_set_up_password"
            android:textColor="@color/common_loan_personal"
            android:textSize="@dimen/common_20dp" />


        <ImageView
            android:layout_width="@dimen/common_20dp"
            android:layout_height="@dimen/common_20dp"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:paddingLeft="@dimen/common_10dp"
            android:src="@drawable/bg_personal_right_selector" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/set_up_sign_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/common_10dp"
        android:background="@drawable/bg_fillet_shadow_selector"
        android:padding="@dimen/common_18dp">

        <com.jsy.jsydemo.control.MyTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:text="@string/name_loan_set_up_sign_out"
            android:textColor="@color/common_loan_personal"
            android:textSize="@dimen/common_20dp" />


        <ImageView
            android:layout_width="@dimen/common_20dp"
            android:layout_height="@dimen/common_20dp"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:paddingLeft="@dimen/common_10dp"
            android:src="@drawable/bg_personal_right_selector" />

    </RelativeLayout>


</LinearLayout>
