                        <com.jsy.jsydemo.control.MyTextView
                            android:id="@+id/loan_basic_life"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginRight="@dimen/common_10dp"
                            android:text="@string/name_loan_credit_please_select"
                            android:textColor="@color/common_loan_personal"
                            android:textSize="@dimen/common_14dp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginRight="@dimen/common_5dp"
                            android:src="@drawable/bg_personal_right_selector" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/common_1dp"
                        android:layout_marginRight="@dimen/common_15dp"
                        android:background="@color/common_light_grey" />
                </LinearLayout>

            </LinearLayout>

        </ScrollView>

        <com.jsy.jsydemo.control.MyButton
            android:id="@+id/loan_basic_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@color/common_basic_red"
            android:padding="@dimen/common_10dp"
            android:text="@string/name_loan_basic_submit"
            android:textColor="@color/white"
            android:textSize="@dimen/common_14dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@id/loan_basic_button"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/common_20dp"
            android:layout_marginLeft="@dimen/common_10dp"
            android:layout_marginTop="@dimen/common_10dp"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/loan_basic_image_image"
                style="@style/MyCheckBox_basic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/common_10dp"
                android:text="@string/name_loan_details_basic_agree"
                android:textColor="@color/black"
                android:textSize="@dimen/common_12dp" />

            <com.jsy.jsydemo.control.MyTextView
                android:id="@+id/loan_details_basic_loan_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/common_10dp"
                android:text="@string/name_loan_details_basic_loan"
                android:textColor="@color/common_blue_details"
                android:textSize="@dimen/common_12dp" />

        </LinearLayout>

    </RelativeLayout>
</LinearLayout>
