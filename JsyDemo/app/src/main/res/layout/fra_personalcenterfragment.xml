<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/common_light_grey"
    android:orientation="vertical">


    <include
        layout="@layout/tab_activity_fra"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/ic_personal_src"
        android:padding="@dimen/common_20dp">

        <ImageView
            android:id="@+id/personal_camera"
            android:layout_width="@dimen/common_50dp"
            android:layout_height="@dimen/common_50dp"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/common_15dp"
            android:src="@mipmap/ic_personal_camera"
            tools:ignore="RtlHardcoded" />

        <com.jsy.jsydemo.control.MyTextView
            android:id="@+id/personal_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/common_30dp"
            android:layout_toRightOf="@id/personal_camera"
            android:text="@string/name_loan_personal_logn"
            android:textColor="@color/white"
            android:textSize="@dimen/common_16dp"
            tools:ignore="RelativeOverlap,RtlHardcoded" />

        <ImageView
            android:id="@+id/personal_logo_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginRight="@dimen/common_10dp"
            android:padding="@dimen/common_10dp"
            android:src="@drawable/bg_personal_right_selector"
            tools:ignore="RtlHardcoded" />

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <RelativeLayout
                android:id="@+id/personal_loan_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_personal_data"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_data"
                    android:text="@string/name_loan_personal_data"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/personal_loan_inviting_friends"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_inviting_friends"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_personal_inviting_friends"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_inviting_friends"
                    android:text="@string/name_loan_personal_inviting_friends"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/personal_loan_help_center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_help_center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_personal_help_center"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_help_center"
                    android:text="@string/name_loan_personal_help_center"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/personal_loan_feedback"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_feedback"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_personal_feedback"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_feedback"
                    android:text="@string/name_loan_personal_feedback"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/personal_Loan_record"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_Loan_record_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_loan_record"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_Loan_record_image"
                    android:text="@string/name_loan_personal_Loan_record"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

            <RelativeLayout
                android:visibility="gone"
                android:id="@+id/personal_Loan_brose"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_Loan_brose_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_loan_record"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_Loan_brose_image"
                    android:text="@string/name_loan_personal_Loan_brows"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/personal_loan_setup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/common_15dp"
                android:background="@drawable/bg_fillet_shadow_selector"
                android:padding="@dimen/common_10dp">

                <ImageView
                    android:id="@+id/personal_setup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_18dp"
                    android:src="@mipmap/ic_personal_setup"
                    tools:ignore="RtlHardcoded" />

                <com.jsy.jsydemo.control.MyTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="@dimen/common_10dp"
                    android:layout_toRightOf="@id/personal_setup"
                    android:text="@string/name_loan_personal_setup"
                    android:textColor="@color/common_loan_personal"
                    android:textSize="@dimen/common_14dp"
                    tools:ignore="RelativeOverlap,RtlHardcoded" />


                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerInParent="true"
                    android:layout_marginRight="@dimen/common_20dp"
                    android:padding="@dimen/common_10dp"
                    android:src="@drawable/bg_personal_right_selector"
                    tools:ignore="RtlHardcoded" />
            </RelativeLayout>

        </LinearLayout>
    </ScrollView>

</LinearLayout>
